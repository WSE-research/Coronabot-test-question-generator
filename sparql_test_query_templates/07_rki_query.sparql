PREFIX qa: <http://www.wdaqua.eu/qa#>
PREFIX oa: <http://www.w3.org/ns/openannotation/core/>
ASK
WHERE {
    GRAPH ?GRAPH {
        ?s a qa:AnnotationOfQuery .
        ?s oa:hasBody ?query .
        ?s oa:hasTarget ?location_key .
        ?s oa:annotatedBy ?component .
        ?s oa:annotatedAt ?time .
    }
}
